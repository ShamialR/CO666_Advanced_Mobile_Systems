<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymedicinetodolist">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyMedicineToDoList">
        <activity android:name="com.example.mymedicinetodolist.splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mymedicinetodolist.ReminderActivity" />
        <activity android:name="com.example.mymedicinetodolist.MainActivity">

        </activity>

        <receiver android:name="com.example.mymedicinetodolist.AlarmBroadcast" />
    </application>

</manifest>